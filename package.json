{
  "name": "Vivaldi Tree Tabs",
  "private": true,
  "version": "0.1.0",
  "description": "Vivaldi tree tab extension",
  "scripts": {
    "start": "npm run dev:chrome",
    "build": "npm run build:chrome",
    "build:chrome": "gulp pack --production --vendor=chrome",
    "dev": "npm run dev:chrome",
    "dev:chrome": "gulp --watch --vendor=chrome",
    "lint": "standard",
    "browser": "vivaldi-snapshot http://localhost:2012 --load-extension=dist/extension --remote-debugging-port=2012 --flag-switches-begin --debug-packed-apps --silent-debugger-extension-api --flag-switches-end",
    "kill-browser": "pkill -f vivaldi-snapshot"
  },
  "standard": {
    "globals": [
      "chrome"
    ]
  },
  "devDependencies": {
    "@types/chrome": "0.0.114",
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-loader": "7.x.x",
    "babel-preset-env": "1.x.x",
    "chai": "^4.3.0",
    "chromereload": "0.x.x",
    "debounce": "1.x.x",
    "del": "3.x.x",
    "gulp": "^3.9.0",
    "gulp-bump": "2.x.x",
    "gulp-cache": "0.x.x",
    "gulp-clean-css": "^3.x.x",
    "gulp-concat": "^2.6.1",
    "gulp-filter": "^5.x.x",
    "gulp-git": "^2.10.1",
    "gulp-if": "2.x.x",
    "gulp-imagemin": "3.x.x",
    "gulp-json-transform": "^0.4.7",
    "gulp-less": "3.x.x",
    "gulp-livereload": "3.x.x",
    "gulp-plumber": "1.x.x",
    "gulp-sass": "^3.x.x",
    "gulp-sequence": "0.x.x",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-tag-version": "1.x.x",
    "gulp-util": "3.x.x",
    "gulp-zip": "^4.x.x",
    "require-dir": "0.x.x",
    "standard": "^10.0.2",
    "ts-loader": "^2.x.x",
    "tslint": "^5.20.1",
    "vinyl-named": "1.x.x",
    "webpack": "3.x.x",
    "webpack-stream": "3.x.x",
    "yargs": "^8.x.x"
  },
  "dependencies": {
    "gulp-open": "^3.0.1",
    "npm-remote-ls": "^1.3.2",
    "typescript": "^3.9.7"
  }
}
