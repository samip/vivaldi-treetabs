{
  "name": "__MSG_appName__",
  "short_name": "__MSG_appShortName__",
  "description": "__MSG_appDescription__",
  "version": "0.0.0",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "default_locale": "en",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "externally_connectable" : {
    "matches": [ "http://localhost/*" ],
    "ids": [ "mpognobbkildjkofajifpdfhcoklimli" ]
  },
  "background": {
    "scripts": [
      "scripts/Init.js"
    ]
  },
  "commands": {
    "close-child-tabs": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Close current tab's children"
    }
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "storage",
    "<all_urls>",
    "management"
  ]
}
